{
  "name": "pandora-cloud-api",
  "version": "1.1.2",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.11.2",
    "aws-sdk": "^2.901.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^16.2.0",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "node-cache": "^5.1.2",
    "rimraf": "^3.0.2",
    "sequelize": "^6.19.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "release-it": "^14.5.1",
    "sequelize-cli": "^6.2.0",
    "sinon": "^9.2.2",
    "supertest": "^6.0.1"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "jsonFiles/*"
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/index.js",
    "dev": "NODE_ENV=development nodemon --exec npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "pretest": "eslint .",
    "test": "jest --detectOpenHandles --coverage --forceExit",
    "release": "dotenv release-it",
    "sequelize:database1:drop": "sequelize --options-path ./.sequelize-database1 --env database1 db:drop",
    "sequelize:database1:create": "sequelize --options-path ./.sequelize-database1 --env database1 db:create",
    "sequelize:database1:migrate": "sequelize --options-path ./.sequelize-database1 --env database1 db:migrate",
    "sequelize:database1:migrate:undo": "sequelize --options-path ./.sequelize-database1 --env database1 db:migrate:undo",
    "sequelize:database1:seed:all": "sequelize --options-path ./.sequelize-database1 --env database1 db:seed:all",
    "sequelize:database2:drop": "sequelize --options-path ./.sequelize-database2 --env database2 db:drop",
    "sequelize:database2:create": "sequelize --options-path ./.sequelize-database2 --env database2 db:create",
    "sequelize:database2:migrate": "sequelize --options-path ./.sequelize-database2 --env database2 db:migrate",
    "sequelize:database2:migrate:undo": "sequelize --options-path ./.sequelize-database2 --env database2 db:migrate:undo",
    "sequelize:database2:seed:all": "sequelize --options-path ./.sequelize-database2 --env database2 db:seed:all",
    "sequelize:database3:drop": "sequelize --options-path ./.sequelize-database3 --env database3 db:drop",
    "sequelize:database3:create": "sequelize --options-path ./.sequelize-database3 --env database3 db:create",
    "sequelize:database3:migrate": "sequelize --options-path ./.sequelize-database3 --env database3 db:migrate",
    "sequelize:database3:migrate:undo": "sequelize --options-path ./.sequelize-database3 --env database3 db:migrate:undo",
    "sequelize:database3:seed:all": "sequelize --options-path ./.sequelize-database3 --env database3 db:seed:all",
    "sequelize:database4:drop": "sequelize --options-path ./.sequelize-database4 --env database4 db:drop",
    "sequelize:database4:create": "sequelize --options-path ./.sequelize-database4 --env database4 db:create",
    "sequelize:database4:migrate": "sequelize --options-path ./.sequelize-database4 --env database4 db:migrate",
    "sequelize:database4:migrate:undo": "sequelize --options-path ./.sequelize-database4 --env database4 db:migrate:undo",
    "sequelize:database4:seed:all": "sequelize --options-path ./.sequelize-database4 --env database4 db:seed:all",
    "sequelize:database5:drop": "sequelize --options-path ./.sequelize-database5 --env database5 db:drop",
    "sequelize:database5:create": "sequelize --options-path ./.sequelize-database5 --env database5 db:create",
    "sequelize:database5:migrate": "sequelize --options-path ./.sequelize-database5 --env database5 db:migrate",
    "sequelize:database5:migrate:undo": "sequelize --options-path ./.sequelize-database5 --env database5 db:migrate:undo",
    "sequelize:database5:seed:all": "sequelize --options-path ./.sequelize-database5 --env database5 db:seed:all",
    "dropDatabases": "npm-run-all sequelize:database1:drop sequelize:database2:drop sequelize:database3:drop sequelize:database4:drop sequelize:database5:drop",
    "createDatabases": "npm-run-all sequelize:database1:create sequelize:database2:create sequelize:database3:create sequelize:database4:create sequelize:database5:create",
    "migrateDatabases": "npm-run-all sequelize:database1:migrate sequelize:database2:migrate sequelize:database3:migrate sequelize:database4:migrate sequelize:database5:migrate",
    "seedDatabases": "npm-run-all sequelize:database1:seed:all sequelize:database2:seed:all sequelize:database3:seed:all sequelize:database4:seed:all sequelize:database5:seed:all",
    "rebuildDatabases": "npm-run-all dropDatabases createDatabases migrateDatabases seedDatabases"
  }
}
